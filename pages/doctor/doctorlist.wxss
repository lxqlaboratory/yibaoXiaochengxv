.searchtab {
  width: 100%;
  height: 6vh;
  background-color: #7acfa6;
  box-sizing: border-box;
  border-top: 1px solid gainsboro;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
}

.searchtab image {
  height: 3.5vh;
  width: 3.5vh;
}

.input_college {
  box-sizing: border-box;
  height: 3.5vh;
  color: black;
  background-color: white;
  min-width: 45%;
  border-radius: 2vh;
  padding-left: 10px;
  font-size: 13px;
}

.myplaceholder {
  font-size: 12px;
}

.input_stu {
  font-size: 13px;
  box-sizing: border-box;
  height: 3.5vh;
  color: black;
  background-color: white;
  max-width: 40%;
  border-radius: 2vh;
  padding-left: 10px;
}

scroll-view {
  height: 94vh;
  width: 100%;
}

.content {
  width: 100%;
}

.part_s{
  background-color: gainsboro !important;
}
.collegetitle {
  margin-top: 15px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 5px 15px 5px 15px;
}

.tba {
  flex: 1;
  background-color: #7acfa6;
  height: 1px;
}

.ctitle {
  box-sizing: border-box;
  padding: 0 15px 0 15px;
  color: green;
}

.doctoritem {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 8px;
  box-sizing: border-box;
  background-color: white;
  box-shadow: 1px 1px 3px  gray;
  margin-bottom: 5px;
}

.doctoritem image {
  height: 14vw;
  width: 14vw;
  border-radius: 50%;
  margin-right: 12px;
  
}
.stuname{
  color: black;
  font-size: 14px;
}
.stunum{
  font-size: 12px;
  line-height: 150%;
}